{
  "name": "vox-populi",
  "version": "1.0.0",
  "description": "An encrypted conflict-free replicated datatype system for counting votes",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist",
    "test": "jest --coverage --roots ./tests",
    "test:watch": "jest --watch --roots ./tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npetrangelo/vox-populi.git"
  },
  "keywords": ["voting", "democracy"],
  "author": "Nathan Petrangelo <nathan@petrangelo.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/npetrangelo/vox-populi/issues"
  },
  "homepage": "https://github.com/npetrangelo/vox-populi#readme",
  "dependencies": {
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^7.10.0",
    "babel-cli": "^6.26.0",
    "jest": "^28.1.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.11.0",
    "typescript": "^4.6.4"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/src"
  ]
