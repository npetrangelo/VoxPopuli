  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@types/prettier": "^2.6.3",
    "babel-cli": "^6.26.0",
    "dotenv": "^16.0.1",
    "jest": "^28.1.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tslint": "^6.1.3",
